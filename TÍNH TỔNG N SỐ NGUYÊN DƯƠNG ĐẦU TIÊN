import java.util.Scanner;

public class Main {


    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        long tc = sc.nextInt();
        while (tc-- > 0) {
            long n = sc.nextLong();
            long res = n*(n + 1) / 2;
            System.out.println(res);
        }
        sc.close();
    }
}
